<div class="dingtalk-content {% if sync_mode != 'dingtalk' %}hidden{% endif %}">
      <div class="form-group" id="dingtalk_tips">
        <div class="col-md-3 control-label">
          <label for=""></label>
        </div>
        <div class="controls col-md-8">
          <div class="help-block" id="dingtalk_sync_tips">
           {{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips'|trans|raw }}
            <p class="color-danger">{{ 'admin.user_setting.account_synchronization.to_admin_org_tips'|trans({'%path%':path('admin_org')})|raw }}</p>
          </div>
          <div class="controls" >
            <p>{{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips.first'|trans|raw }}</p>
            <p class="ptm">{{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips.one'|trans|raw }}</p>
          </div>
          <div class="help-block" >
            <p class="ptm">{{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips.two'|trans|raw }}</p>
            <p class="ptm">{{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips.host'|trans({'%host%': host})|raw }}</p>
            <p class="ptm">{{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips.ip'|trans({'%ip%': ip})|raw }}</p>
          </div>
        </div>
      </div>
      <div id="dingtalk_app_setting">
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for=""></label>
          </div>
          <div class="controls col-md-9">
            <p>{{'admin.user_setting.account_synchronization.dingtalk_sync_tips.h5'|trans}}</p>
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for="dingtalksync_agentId">AgentId</label>
          </div>
          <div class="controls col-md-8">
            <input type="text" id="dingtalksync_agentId" name="dingtalksync_agentId" class="form-control" value="{{ syncSetting.agentId|default() }}">
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for="dingtalksync_key">AppKey</label>
          </div>
          <div class="controls col-md-8">
            <input type="text" id="dingtalksync_key" name="dingtalksync_key" class="form-control" value="{{ syncSetting.key|default() }}">
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for="dingtalksync_secret">AppSecret</label>
          </div>
          <div class="controls col-md-8">
            <input type="text" id="dingtalksync_secret" name="dingtalksync_secret" class="form-control" value="{{ syncSetting.secret|default() }}">
          </div>
        </div>
        {% if syncSetting.key|default() and  syncSetting.secret|default() %}
          <div class="form-group">
            <div class="controls col-md-offset-3 col-md-8">
              <button type="button" class="btn submit btn-primary js-check-btn" data-url="{{ path('dingtalk_check_sync_setting') }}">{{'form.btn.verify'|trans}}</button>
            </div>
          </div>
        {% endif %}
      </div>
       <div id="dingtalk_corp_setting">
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for=""></label>
          </div>
          <div class="controls col-md-9">
            <span>{{ 'admin.user_setting.account_synchronization.dingtalk_sync_tips.second'|trans|raw }}</span>
            <div class="help-block">
              {{ 'admin.login_connect.weixinmob.message.tips_five'|trans|raw }}
            </div>
          </div>

        </div>
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for="dingtalkweb_key">AppID</label>
          </div>
          <div class="controls col-md-8">
            <input type="text" id="dingtalkweb_key" name="dingtalkweb_key" class="form-control" value="{{ loginConnect.dingtalkweb_key|default() }}">
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-3 control-label">
            <label for="dingtalkweb_secret">AppSecret</label>
          </div>
          <div class="controls col-md-8">
            <input type="text" id="dingtalkweb_secret" name="dingtalkweb_secret" class="form-control" value="{{ loginConnect.dingtalkweb_secret|default() }}">
          </div>
        </div>
      </div>
    </div>
